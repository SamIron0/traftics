#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/samuelironkwe/Documents/session-recorder/node_modules/.pnpm/ts-jest@29.2.5_@babel+core@7.26.0_esbuild@0.17.19_jest@29.7.0_typescript@5.6.3/node_modules/ts-jest/node_modules:/Users/samuelironkwe/Documents/session-recorder/node_modules/.pnpm/ts-jest@29.2.5_@babel+core@7.26.0_esbuild@0.17.19_jest@29.7.0_typescript@5.6.3/node_modules:/Users/samuelironkwe/Documents/session-recorder/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/samuelironkwe/Documents/session-recorder/node_modules/.pnpm/ts-jest@29.2.5_@babel+core@7.26.0_esbuild@0.17.19_jest@29.7.0_typescript@5.6.3/node_modules/ts-jest/node_modules:/Users/samuelironkwe/Documents/session-recorder/node_modules/.pnpm/ts-jest@29.2.5_@babel+core@7.26.0_esbuild@0.17.19_jest@29.7.0_typescript@5.6.3/node_modules:/Users/samuelironkwe/Documents/session-recorder/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
